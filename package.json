{
  "name": "dayz-leaderboard-bot",
  "description": "A DayZ bot written in Javascript to display your leaderboard using the CFTools Cloud API.",
  "version": "0.0.1",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:dev": "nodemon run node --trace-warnings .",
    "start": "pm2 start src/index.js --name=dayz-leaderboard-bot",
    "stop": "pm2 stop dayz-leaderboard-bot",
    "remove": "pm2 stop dayz-leaderboard-bot && pm2 delete dayz-leaderboard-bot && pm2 reset dayz-leaderboard-bot",
    "logs": "pm2 logs --lines 300 dayz-leaderboard-bot",
    "logsError": "pm2 logs --err --lines 300 dayz-leaderboard-bot",
    "lint": "eslint src",
    "linter": "eslint src --fix",
    "writeLinter": "eslint src --output-file linter-output.txt"
  },
  "engines": {
    "node": ">=16.10.0"
  },
  "dependencies": {
    "@discordjs/rest": "^0.4.1",
    "@mirasaki/logger": "^1.0.5",
    "cftools-sdk": "^1.8.0",
    "common-tags": "^1.8.2",
    "discord.js": "^14.1.2",
    "dotenv": "^16.0.0",
    "express": "^4.18.1"
  },
  "devDependencies": {
    "eslint": "^8.15.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "nodemon": "^2.0.16"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mirasaki/dayz-leaderboard-bot.git"
  },
  "keywords": [
    "bot-template",
    "template",
    "discord",
    "discord-bot",
    "discord-bot-template"
  ],
  "author": "Richard Hillebrand (Mirasaki)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mirasaki/dayz-leaderboard-bot/issues"
  },
  "homepage": "https://github.com/Mirasaki/dayz-leaderboard-bot#readme"
}
