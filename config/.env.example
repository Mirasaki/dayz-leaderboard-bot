# Required variables
NODE_ENV=production
DISCORD_BOT_TOKEN=
CLIENT_ID=
TEST_SERVER_GUILD_ID=

# Automatic leaderboard posting
# ONLY supported for the first server entry
# when using multiple server setup
AUTO_LB_ENABLED=true
AUTO_LB_CHANNEL_ID=
AUTO_LB_INTERVAL_IN_MINUTES=60
AUTO_LB_REMOVE_OLD_MESSAGES=true
AUTO_LB_PLAYER_LIMIT=100

# (OPTIONAL) API port to check if process is alive
# See the express code in src/index.js for more information
PORT=

# CFTools Cloud API & GameLabs
CFTOOLS_API_APPLICATION_ID=
CFTOOLS_API_SECRET=
# Amount of players to display on the leaderboard. 10 min, 25 max
CFTOOLS_API_PLAYER_DATA_COUNT=15

# Debug variables
DEBUG_ENABLED=true
DEBUG_SLASH_COMMAND_API_DATA=true
DEBUG_INTERACTIONS=false
DEBUG_AUTOCOMPLETE_RESPONSE_TIME=true
DEBUG_LEADERBOARD_API_DATA=false
DEBUG_STAT_COMMAND_DATA=false

# Interaction command API data
REFRESH_SLASH_COMMAND_API_DATA=true
CLEAR_SLASH_COMMAND_API_DATA=false